 Parallax } from 'react-scroll-parallax';\nimport projects from '../data/projectData.js';\nimport { useEffect, useState } from 'react';\nimport Fade from 'react-reveal/Fade';\n\n\n\nconst myLoader = ({ src, width, quality }) => {\n  return `https://d13gv6jsteq2nf.cloudfront.net/${src}?w=${width}&q=${quality || 75}`\n}\n\nconst Project = (props) => {\n  const [ hover, setHover ] = useState(true);\n  const toggleHover = () => {\n    setHover(!hover);\n  };\n  return (\n    <div>\n      {props.data.left ?\n      <div class=\"project\">\n        <div class=\"highlightImage\">\n          <Fade distance=\"5vh\" delay={500} bottom>\n            <Image\n            loader={myLoader}\n            src={`${props.data.images[0]}.png`}\n            alt=\"highlight\"\n            width=\"600\"\n            height=\"337.5\"\n            />\n          </Fade>\n        </div>\n\n        <div class=\"projectDescription\">\n          <Fade distance=\"5vh\" delay={500} bottom>\n            <h3>{props.data.name}</h3>\n          </Fade>\n          <Fade distance=\"5vh\" delay={700} bottom>\n            <p>{props.data.tech}</p>\n          </Fade>\n\n\n        <div>\n\n          <Fade distance=\"5vh\" delay={900} bottom>\n            <div>\n              <Link href={`/projects/${props.data.link}`}>\n                <div class={hover ? \"send\" : \"sendFill\"} onMouseEnter={toggleHover} onMouseLeave={toggleHover}>VIEW PROJECT</div>\n              </Link>\n\n            </div>\n          </Fade>\n\n        </div>\n\n      </div>\n    </div> :\n    <div class=\"projectRight\">\n      <div class=\"projectDescription\">\n        <Fade distance=\"5vh\" delay={500} bottom>\n          <h3>{props.data.name}</h3>\n        </Fade>\n        <Fade distance=\"5vh\" delay={700} bottom>\n          <p>{props.data.tech}</p>\n        </Fade>\n        <div>\n          <Fade distance=\"5vh\" delay={900} bottom>\n            <div>\n              <Link href={`/projects/${props.data.link}`}>\n                <div class={hover ? \"send\" : \"sendFill\"} onMouseEnter={toggleHover} onMouseLeave={toggleHover}>VIEW PROJECT</div>\n              </Link>\n            </div>\n          </Fade>\n        </div>\n\n\n        </div>\n          <div class=\"highlightImageRight\">\n            <Fade distance=\"5vh\" delay={500} bottom>\n              <Image\n              loader={myLoader}\n              src={`${props.data.images[0]}.png`}\n              alt=\"highlight\"\n              width=\"600\"\n              height=\"337.5\"\n              />\n            </Fade>\n          </div>\n        </div>\n       }\n    </div>\n  )\n}\n\n\n\n\nconst Projects = () => {\n\n  useEffect(() => {\n    document.body.style.overflow = 'visible'\n  });\n\n  const data = projects.data;\n  const list = data.map(project => {\n    return <Project data={project} key={data.indexOf(project)} />\n  });\n\n  return (\n    <div>\n      <ParallaxProvider>\n       <div class=\"projects\">\n        <Fade distance=\"5vh\" delay={500} bottom>\n          <h1>Featured Projects</h1>\n        </Fade>\n\n        <div class=\"image1\">\n          <Parallax y={[-10, 10]}>\n            <Fade distance=\"5vh\" delay={900} bottom>\n              <Image\n                loader={myLoader}\n                src=\"birds.jpg\"\n                alt=\"birds\"\n                width=\"640\"\n                height=\"640\"\n                />\n            </Fade>\n          </Parallax>\n        </div>\n\n        <div class=\"image2\">\n          <Parallax y={[-15, 15]}>\n\n            <Fade distance=\"5vh\" delay={700} bottom>\n              <Image\n                loader={myLoader}\n                class=\"image2\"\n                src=\"desk.jpg\"\n                alt=\"desk\"\n                width=\"640\"\n                height=\"640\"\n                />\n            </Fade>\n           </Parallax>\n\n\n        </div>\n\n        <div class=\"image1\">\n          <Parallax y={[20, -20]}>\n              <Fade distance=\"5vh\" delay